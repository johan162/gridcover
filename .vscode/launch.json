    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "gridcover 50%(10x10)",
            "cargo": {
                "args": [
                    "build",
                    "--bin=gridcover",
                    "--package=gridcover"
                ],
                "filter": {
                    "name": "gridcover",
                    "kind": "bin"
                }
            },
            "args": [
                "-W",
                "10",
                "-H",
                "10",
                "-c",
                "50",
                "-R",
                "false"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "gridcover 1 (5x5)",
            "cargo": {
                "args": [
                    "build",
                    "--bin=gridcover",
                    "--package=gridcover"
                ],
                "filter": {
                    "name": "gridcover",
                    "kind": "bin"
                }
            },
            "args": [
                "-W",
                "10",
                "-H",
                "10",
                "-m",
                "1",
                "-R",
                "false"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "mapex01",
            "cargo": {
                "args": [
                    "build",
                    "--bin=gridcover",
                    "--package=gridcover"
                ],
                "filter": {
                    "name": "gridcover",
                    "kind": "bin"
                }
            },
            "args": [
                "-M",
                "assets/mapex01.yaml",
                "-o",
                "coverage.png",
                "--show-quad-tree",
                "true",
                "-S",
                "223",
                "-m",
                "2",
                "--start-x",
                "2",
                "--start-y",
                "6",
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "mapex02",
            "cargo": {
                "args": [
                    "build",
                    "--bin=gridcover",
                    "--package=gridcover"
                ],
                "filter": {
                    "name": "gridcover",
                    "kind": "bin"
                }
            },
            "args": [
                "-W",
                "7",
                "-H",
                "7",
                "-M",
                "assets/mapex02.yaml",
                "-o",
                "coverage.png",
                "-G",
                "true",
                "-S",
                "457",
                "-s",
                "0.01",
                "-x",
                "0",
                "-y",
                "0",
                "-J",
                "true",
                "--dir-x",
                "1",
                "--dir-y",
                "1",
                "-b",
                "10"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'gridcover'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=gridcover",
                    "--package=gridcover"
                ],
                "filter": {
                    "name": "gridcover",
                    "kind": "bin"
                }
            },
            "args": [
                "-W",
                "10",
                "-H",
                "10",
                "-c",
                "50",
                "-R",
                "false"
            ],
            "cwd": "${workspaceFolder}"
        }
    ]
